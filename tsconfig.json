{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "removeComments": false,
    "declaration": true,
    "outDir": "generated",
    "sourceMap": false,
    "noImplicitReturns": true
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "generated"
  ]
}
